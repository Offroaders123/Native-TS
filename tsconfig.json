{
  "extends": "assemblyscript/std/portable.json",
  "compilerOptions": {
    "module": "ESNext",
    "noEmit": true,
    "strict": true,
    "noImplicitOverride": true,
    "noPropertyAccessFromIndexSignature": true,
    "noUncheckedIndexedAccess": true,
    "allowUnreachableCode": false,
    "noUnusedLocals": true,
    "noUnusedParameters": true
  }
}